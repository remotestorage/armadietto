{
  "host_identity": "localhost",
  "basePath": "",
  "allow_signup": true,
  "http": {
    "host": "0.0.0.0",
    "port": 8000
  },
  "https": {
    "host": "0.0.0.0",
    "port": 4443,
    "enable": true,
    "force": false,
    "cert": "/etc/armadietto/cert.pem",
    "key": "/etc/armadietto/privkey.pem"
  },
  "folder_items_contain_type": true,
  "trust_proxy": null,
  "logging": {
	  "log_dir": "logs",
	  "stdout": ["info"],
	  "log_files": ["notice"]
  }
}
